<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>
  
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
  	<servlet-name>CosUploadServlet</servlet-name>
  	<servlet-class>cn.hao.sevrlet.CosUploadServlet</servlet-class>
  </servlet>
  <servlet>  
        <servlet-name>CosDownloadServlet</servlet-name>  
        <servlet-class>cn.hao.sevrlet.CosDownloadServlet</servlet-class>  
    </servlet> 
  <servlet-mapping>
  	<servlet-name>CosUploadServlet</servlet-name>
  	<url-pattern>/CosUploadServlet</url-pattern>
  </servlet-mapping>
   <servlet-mapping>  
        <servlet-name>CosDownloadServlet</servlet-name>  
        <url-pattern>/CosDownloadServlet</url-pattern>  
    </servlet-mapping>
</web-app>
